{
  "version": "2.0",
  "envId": "trade-view-0gtiozig72c07cd0",
  "framework": {
    "name": "vite",
    "config": {
      "buildCommand": "cd frontend && npm install && npm run build",
      "outputPath": "frontend/dist"
    }
  },
  "hosting": {
    "type": "static",
    "cloudPath": "/",
    "ignore": [
      "node_modules/**",
      ".git/**",
      "*.md",
      "backend/**",
      "functions/**"
    ],
    "rewriteRules": [
      {
        "match": ".*",
        "to": "/index.html"
      }
    ]
  },
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "trading-api",
      "timeout": 60,
      "memorySize": 512,
      "runtime": "Python3.9",
      "handler": "index.main_handler",
      "installDependency": true,
      "ignore": [
        "node_modules/**",
        ".git/**",
        "*.md",
        "data/**",
        "scripts/**",
        "tests/**",
        "__pycache__/**",
        "*.pyc",
        ".pytest_cache/**"
      ]
    }
  ]
}
